<Window x:Class="gestadh45.Main.UCWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GestAdh45" Height="400" Width="600" SizeToContent="WidthAndHeight" Icon="/gestadh45.Main;component/Icones/gestadh45.ico">
	
	<Window.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Skins/MainSkin.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Window.Resources>

	<Grid>
		<Border Name="contenu" Margin="5,5,5,5" BorderThickness="1" BorderBrush="#FF000000" Style="{StaticResource StyleEnfant}" />
	</Grid>
</Window>
