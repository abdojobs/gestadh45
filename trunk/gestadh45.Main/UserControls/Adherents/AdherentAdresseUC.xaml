<UserControl x:Class="gestadh45.Main.UserControls.Adherents.AdherentAdresseUC"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 x:Name="AdherentAdresse"
			 mc:Ignorable="d" 
			 d:DesignHeight="100" d:DesignWidth="200">
	<GroupBox Header="Adresse" Margin="0,0,0,10" Background="#FFFFDEAD" BorderBrush="#FF000000">
		<StackPanel Orientation="Vertical">
			<TextBlock Text="{Binding ElementName=AdherentAdresse, Path=Adherent.Adresse}" />
			<TextBlock Text="{Binding ElementName=AdherentAdresse, Path=Adherent.Ville}" />
		</StackPanel>
	</GroupBox>
</UserControl>
