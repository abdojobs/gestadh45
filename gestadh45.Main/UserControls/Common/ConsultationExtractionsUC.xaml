<UserControl x:Class="gestadh45.Main.UserControls.Common.ConsultationExtractionsUC"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 mc:Ignorable="d" 
			 DataContext="{Binding ConsultationExtractionsVMStatic, Source={StaticResource Locator}}" 
			 d:DesignHeight="300" d:DesignWidth="300" Width="300" Height="300">
	<DockPanel Margin="5,5,5,5">
		<Button ToolTip="Annuler" DockPanel.Dock="Bottom" HorizontalAlignment="Center" 
				Width="32" Height="32" Margin="0,10,0,10" 
					CommandParameter=""
					Command="{Binding AnnulerCommand}">
			<Image Source="/gestadh45.Main;component/Images/btn_annuler.png" />
		</Button>

		<TextBox Text="{Binding Path=ResultatExtraction}" TextWrapping="Wrap" />
	</DockPanel>
</UserControl>
